let build(headers, code, data) = do
    let output = []
    output.append("HTTP/1.1 "+string(code)+" OK")
    let keys = headers.keys()
    for (i from 0 to keys.length) do
        output.append(keys[i] + ": " + headers[keys[i]])
    output.append("")
    output.append(data)
    return output.join("\r\n")